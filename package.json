{
  "name": "entrena-con-ia",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "npm --prefix backend run dev",
    "dev:all": "concurrently -n FRONT,API -c green,cyan \"npm run dev\" \"npm run dev:backend\"",
    "install:all": "npm install && npm --prefix backend install",
    "lint": "eslint --config .eslint.config.mjs .",
    "build": "vite build",
    "test": "echo \"⚠️  Tests not configured yet\" && exit 0",
    "postinstall": "husky install 2>/dev/null || true",
    "pre-commit": "lint-staged",
    "preview": "vite preview"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.61.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.510.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.1",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "vite": "^6.3.5",
    "xlsx": "^0.18.5",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "concurrently": "^9.0.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "terser": "^5.44.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --config .eslint.config.mjs --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "backend/**/*.js": [
      "node -c"
    ]
  }
}
